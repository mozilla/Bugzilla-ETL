{
		"source":{
			"host":"http://elasticsearch-private.bugs.scl3.mozilla.com",
			"port":9200,
			"index":"private_bugs",
			"type":"bug_version",
            "explore_metadata":false
		},
		"destination":{
			"host":"http://localhost",
			"port": 9200,
			"index":"bug_hierarchy",
			"type":"bug_version"
		},
		"param":{
			"last_replication_time":"./results/data/last_dependencies_replication_time.txt"
		},
		"debug":{
			"log":[{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "./results/logs/replication.log",
				"maxBytes": 10000000,
				"backupCount": 200,
				"encoding": "utf8"
			},{
				"log_type": "stream",
				"stream":"sys.stdout"
			}]
	}


}
